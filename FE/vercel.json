{
  "rewrites": [
    {
      "source": "/users/(.*)",
      "destination": "${process.env.BACKEND_URL}/users/$1"
    },
    {
      "source": "/job_applications/(.*)",
      "destination": "${process.env.BACKEND_URL}/job_applications/$1"
    },
    {
      "source": "/job_cycles",
      "destination": "${process.env.BACKEND_URL}/job_cycles"
    },
    {
      "source": "/job_cycles/(.*)",
      "destination": "${process.env.BACKEND_URL}/job_cycles/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
